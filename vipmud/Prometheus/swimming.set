#Trigger {*climb*down the steps into the pool.} {
#If {@SilentMode = 0} {
#If {%IfWord(You,%1," ")} {
#If {@GagSwimming = 1} {
#gagline ALL}};
#Play {Prometheus\General\Swimming\swim.wav} @Vol;
#If {@PanSwimming = 1} {
#PC %PlayHandle Pan %random(-4000,4000)}}}
