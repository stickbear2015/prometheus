#If {@SilentMode = 0} {
#If {%IfWord(You,%1," ")} {
#If {@GagSwimming = 1} {
#gagline ALL}};
#Play {Prometheus\general\swimming\swim*5.wav} @vol;
#If {@PanSwimming = 1} {
#PC %PlayHandle Pan %random(-4000,4000)}}}
#Trigger {You push*into the pool!} {
#If {@SilentMode = 0} {
#If {@GagSwimming = 1} {
