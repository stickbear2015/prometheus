#read 6 GagOtherCombat 33;
#Read 6 PlayFootsteps 34;
#Read 6 GagFootsteps 35;
#Read 6 GagSwimming 36;
#Read 6 PanSwimming 37;
#read 6 FlyType 38;
#read 6 Channels 39;
#read 6 Devices 40;
#read 6 Footsteps 41;
#read 6 GagMode 42;
#read 6 QuotationToggle 43;
#read 6 PipeToggle 44;
#Close 6};
#alias LoadClasses {
#If {@SilentMode = 0} {
#class global enable};
#If {@RedAlert = 1} {
#class RedAlert enable};
#If {@Socials = 1} {
#class socials enable};
#If {@RoomSounds = 1} {
#class ambiences enable};
#If {@Footsteps = 1} {
#class footsteps enable};
#If {@Channels = 1} {
#class channels enable};
#If {@Devices = 1} {
#class devices enable}}
#say {Welcome to the Ultimate VIPMud soundpack for Prometheus! By the main administrator of Prometheus!}
#Alias {onconnect} {LoadSettings;ContVars;LoadClasses;#If {@RoomSoundPlaying = 1} {#PC @RoomHandle stop; #Var RoomSoundPlaying 0}; #if {@silentmode = 0} {#Play Prometheus\Music\*5.wav @Vol}} {}
#Alias OnDisconnect {#pc 0 stop; #If {@RoomSoundPlaying = 1} {#pc @RoomHandle stop; #Var RoomSoundPlaying 0}; #if {@silentmode = 0} {#Play Prometheus\general\misc\MudDisconnected.wav @Vol};spsave}
#alias spsave {
#File 6 Prometheus/Settings.set 1;#Var SettingsFileCounter 1;
#Write 6 @roomvol @SettingsFileCounter;#Math SettingsFileCounter {@SettingsFileCounter +1};
#Write 6 @Vol @SettingsFileCounter;#Math SettingsFileCounter {@SettingsFileCounter +1};
#Write 6 @SilentMode @SettingsFileCounter;#Math SettingsFileCounter {@SettingsFileCounter +1};
#Write 6 0 @SettingsFileCounter;#Math SettingsFileCounter {@SettingsFileCounter +1};
#Write 6 @RoomSoundPlaying @SettingsFileCounter;#Math SettingsFileCounter {@SettingsFileCounter +1};
#Write 6 @RoomSounds @SettingsFileCounter;#Math SettingsFileCounter {@SettingsFileCounter +1};
#Write 6 @RedAlert @SettingsFileCounter;#Math SettingsFileCounter {@SettingsFileCounter +1};
#Write 6 @GagShipCombat @SettingsFileCounter;#Math SettingsFileCounter {@SettingsFileCounter +1};
#Write 6 @Panning @SettingsFileCounter;#Math SettingsFileCounter {@SettingsFileCounter +1};
