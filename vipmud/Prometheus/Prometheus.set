#alias LoadSettings {
#File 6 Prometheus/Settings.set;
#Read 6 roomvol 1;
#Read 6 vol 2;
#Read 6 SilentMode 3;
#Read 6 RoomHandle 4;
#Read 6 RoomSoundPlaying 5;
#Read 6 RoomSounds 6;
#Read 6 RedAlert 7;
#Read 6 GagShipCombat 8;
#Read 6 Panning 9;
#Read 6 GagShipMoves 10;
#Read 6 GagVehicleMoves 11;
#Read 6 GagVehicleCombat 12;
#Read 6 PanOtherShipMoves 13;
#Read 6 Socials 14;
#Read 6 GagMining 15;
#Read 6 PanMining 16;
#Read 6 GagArch 17;
#Read 6 PanArch 18;
#Read 6 GagDock 19;
#Read 6 PanDock 20;
#Read 6 GagHyper 21;
#Read 6 GagHunt 22;
#Read 6 PanHunt 23;
#Read 6 BeepRT 24;
#Read 6 GagSal 25;
#Read 6 PanSal 26;
#Read 6 GagTrans 27;
#Read 6 PanTrans 28;
#Read 6 GagShiplifts 29;
#Read 6 PanShiplifts 30;
#Read 6 GagVehicles 31;
